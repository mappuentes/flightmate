        android:textColor="@color/black"
        android:textSize="10sp"
        android:textStyle="bold"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/viewVuelo"
        app:layout_constraintEnd_toStartOf="@+id/divider3"
        app:layout_constraintHorizontal_bias="0.075"
        app:layout_constraintStart_toStartOf="@+id/viewVuelo"
        app:layout_constraintTop_toTopOf="@+id/viewVuelo"
        app:layout_constraintVertical_bias="0.842" />

    <TextView
        android:id="@+id/textViewHoraSalida"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textColor="@color/black"
        android:textSize="10sp"
        android:textStyle="bold"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/viewVuelo"
        app:layout_constraintEnd_toStartOf="@+id/divider3"
        app:layout_constraintHorizontal_bias="0.075"
        app:layout_constraintStart_toStartOf="@+id/viewVuelo"
        app:layout_constraintTop_toTopOf="@+id/viewVuelo"
        app:layout_constraintVertical_bias="0.461" />

    <View
        android:id="@+id/viewcabecera"
        android:layout_width="370dp"
        android:layout_height="72dp"
        android:background="@color/gris"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/viewVuelo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.463"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/et_dataInput"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textViewNumero_vuelo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textColor="@color/black"
        android:textSize="10sp"
        android:textStyle="bold"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/viewcabecera"
        app:layout_constraintEnd_toEndOf="@+id/viewcabecera"
        app:layout_constraintHorizontal_bias="0.864"
        app:layout_constraintStart_toStartOf="@+id/viewcabecera"
        app:layout_constraintTop_toTopOf="@+id/viewcabecera"
        app:layout_constraintVertical_bias="0.827" />

    <TextView
        android:id="@+id/textViewStatus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textColor="@color/black"
        android:textSize="18sp"
        android:textStyle="bold"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/viewcabecera"
        app:layout_constraintEnd_toEndOf="@+id/viewcabecera"
        app:layout_constraintStart_toStartOf="@+id/viewcabecera"
        app:layout_constraintTop_toTopOf="@+id/viewcabecera" />

    <TextView
        android:id="@+id/textViewHoraLlegadaprogramada"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textColor="@color/black"
        android:textSize="10sp"
        android:textStyle="bold"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/viewVuelo"
        app:layout_constraintEnd_toEndOf="@+id/viewVuelo"
        app:layout_constraintHorizontal_bias="0.075"
        app:layout_constraintStart_toEndOf="@+id/divider3"
        app:layout_constraintTop_toTopOf="@+id/viewVuelo"
